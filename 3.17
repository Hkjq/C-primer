#include <iostream>
using namespace std;
#include <vector>
#include <string>

int main() {
	vector<char>v;
	string s;
	cin >> s;
	for (int i = 0; i < s.size(); i++)
	{
		v.push_back(toupper(s[i]));
	}
	for (vector<char>::iterator it = v.begin();it!=v.end();it++)
	{
		cout << (*it) << endl;
	}
	cout << endl;
	system("pause");
	return 0;
}
