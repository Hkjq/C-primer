#include <iostream>
using namespace std;
#include <vector>

int main() {
	vector<int>v;
	int num = 0;
	cout << "输入需要添加的数字个数";
	cin >> num;
	int a = 0;
	for (int i = 0; i < num; i++)
	{
		cout << "需要输入的第"<<i+1<<"数字";
		cin >> a;
		v.push_back(a);
	}
	for (vector<int>::iterator it = v.begin();it != v.end();it++)
	{
		cout << (*it) << endl;
	}
	system("pause");
	return 0;
}
